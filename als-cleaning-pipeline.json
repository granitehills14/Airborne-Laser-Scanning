{
    "pipeline": [
        {
           "type":"readers.las"
        },
        {
            "type":"filters.dem",
            "raster":"tandem.tif",
            "limits":"Z[20:35]"
        },
        {
            "type":"filters.outlier",
            "method":"statistical",
            "mean_k":12,
            "multiplier":2.2
        },
        {
          "type":"filters.range",
          "limits":"classification![7:7]"
        },
        {
            "type":"filters.smrf"
        },
        {
            "type":"filters.range",
            "limits":"Classification[2:2]"
        },
        {
           "type":"writers.las"
        },
        {
            "type":"writers.gdal",
            "resolution":1.0,
            "output_type":"idw"
        }
    ]
}
